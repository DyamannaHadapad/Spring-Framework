package com.xworkz.beans.bean;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class Court {

	@Autowired
	private String name;
	private String type;
	private String place;
	private int noOfCourts;
	private Judge judge;

	public Court() {
		System.out.println("defult constr of court.....");
	}

	@Override
	public String toString() {
		return "Court [name=" + name + ", type=" + type + ", place=" + place + ", noOfCourts=" + noOfCourts + ", judge="
				+ judge + "]";
	}
	
	

}
